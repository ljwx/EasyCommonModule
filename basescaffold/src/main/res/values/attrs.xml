<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--多状态根布局-->
    <declare-styleable name="QuickStateRefreshLayout">
        <attr name="qsrl_layout_model" format="integer">
            <enum name="none" value="0" />
            <enum name="just_state" value="1" />
            <enum name="just_refresh" value="2" />
            <enum name="state_refresh" value="3" />
            <enum name="refresh_state" value="4" />
        </attr>
        <attr name="qsrl_layout_loading" format="reference" />
        <attr name="qsrl_layout_empty" format="reference" />
        <attr name="qsrl_layout_error" format="reference" />
        <attr name="qsrl_layout_network" format="reference" />
    </declare-styleable>

</resources>